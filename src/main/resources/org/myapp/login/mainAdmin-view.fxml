<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="442.0" prefWidth="827.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.myapp.login.igu.controller.MainAdminController">
   <children>
      <Label alignment="CENTER" layoutX="5.0" layoutY="42.0" prefHeight="33.0" prefWidth="388.0" text="User Administration System">
         <font>
            <Font name="Yu Gothic Bold" size="27.0" />
         </font>
      </Label>
      <Label fx:id="txtLabel" alignment="CENTER" layoutX="750.0" layoutY="14.0" prefHeight="20.0" prefWidth="63.0" text="Label">
         <font>
            <Font name="Yu Gothic Bold" size="15.0" />
         </font>
      </Label>
      <TableView fx:id="tableView" layoutX="5.0" layoutY="89.0" prefHeight="349.0" prefWidth="615.0">
        <columns>
           <TableColumn fx:id="id" prefWidth="75.0" reorderable="false" sortable="false" text="ID" />
           <TableColumn fx:id="user" prefWidth="75.0" reorderable="false" sortable="false" text="USER" />
           <TableColumn fx:id="rol" prefWidth="75.0" reorderable="false" sortable="false" text="ROL" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="btnCreateUser" layoutX="646.0" layoutY="101.0" mnemonicParsing="false" onAction="#btnCreate" prefHeight="52.0" prefWidth="126.0" text="Create User">
         <font>
            <Font name="Segoe UI Black" size="15.0" />
         </font>
      </Button>
      <Button fx:id="btUpdate" layoutX="646.0" layoutY="169.0" mnemonicParsing="false" onAction="#btnUpdate" prefHeight="52.0" prefWidth="126.0" text="Update User">
         <font>
            <Font name="Segoe UI Black" size="15.0" />
         </font>
      </Button>
      <Button layoutX="646.0" layoutY="241.0" mnemonicParsing="false" onAction="#btnDelete" prefHeight="52.0" prefWidth="126.0" text="Delete User">
         <font>
            <Font name="Segoe UI Black" size="15.0" />
         </font>
      </Button>
      <Button layoutX="646.0" layoutY="308.0" mnemonicParsing="false" onAction="#btnReload" prefHeight="52.0" prefWidth="126.0" text="Reload Table">
         <font>
            <Font name="Segoe UI Black" size="15.0" />
         </font>
      </Button>
      <Button fx:id="btnExit" layoutX="646.0" layoutY="376.0" mnemonicParsing="false" onAction="#btnExit" prefHeight="52.0" prefWidth="126.0" text="Exit">
         <font>
            <Font name="Segoe UI Black" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
